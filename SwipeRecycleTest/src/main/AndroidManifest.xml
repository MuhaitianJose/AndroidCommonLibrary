<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.muhaitain.swiperecycletest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GridMenuActivity"
            android:label="网格形式菜单"/>
        <activity android:name=".LandScapeMenuActivity"
            android:label="垂直菜单"/>
        <activity android:name=".LeftAndRightMenuActivity"
            android:label="左右菜单"/>
        <activity android:name=".LeftMenuActivity"
            android:label="左边菜单"/>
        <activity android:name=".RightMenuActivity"
            android:label="右边菜单"/>
        <activity android:name=".LoadMoreAndRefreshActivity"
            android:label="加载刷新菜单"/>
    </application>

</manifest>